{
    "name": "mobile.template",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite --port=8100",
        "dev:android": "ionic capacitor run android -l --external --target=Pixel5_34",
        "build": "vue-tsc && vite build && ionic build && ionic cap sync",
        "build:android": "npm run build && cd android && gradlew assembleDebug && cd .. && shx cp ./android/app/build/outputs/apk/debug/app-debug.apk ./apks/app-debug.apk ",
        "build:android:release": "npm run build && cd android && gradlew assembleRelease && cd .. && shx cp ./android/app/build/outputs/apk/release/app-release-unsigned.apk ./apks/app-release.apk ",
        "preview": "vite preview",
        "serve:files": "http-server ./.vite-storage -a localhost -p 8200 -c-1.",
        "test:e2e": "cypress run",
        "test:unit": "vitest",
        "lint": "eslint .",
        "assets:build": "npx @capacitor/assets generate --iconBackgroundColor #daebf8 --iconBackgroundColorDark #222222 --splashBackgroundColor #daebf8 --splashBackgroundColorDark #111111",
        "emulator": "start cmd.exe /c emulator -avd Pixel5_34"
    },
    "dependencies": {
        "@capacitor/android": "6.2.0",
        "@capacitor/app": "6.0.2",
        "@capacitor/core": "6.2.0",
        "@capacitor/haptics": "6.0.2",
        "@capacitor/keyboard": "6.0.3",
        "@capacitor/preferences": "^6.0.3",
        "@capacitor/status-bar": "6.0.2",
        "@ionic/vue": "^8.0.0",
        "@ionic/vue-router": "^8.0.0",
        "@mdi/svg": "^7.4.47",
        "fuzzysort": "^2.0.4",
        "ionicons": "^7.0.0",
        "uuid": "^11.0.5",
        "vue": "^3.3.0",
        "vue-router": "^4.2.0",
        "vue-virtual-scroller": "^2.0.0-beta.8"
    },
    "devDependencies": {
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "6.2.0",
        "@vitejs/plugin-legacy": "^5.0.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.3.0",
        "autoprefixer": "^10.4.20",
        "cypress": "^13.5.0",
        "eslint": "^8.35.0",
        "eslint-plugin-vue": "^9.9.0",
        "jsdom": "^22.1.0",
        "postcss": "^8.5.1",
        "sass": "^1.83.4",
        "shx": "^0.3.4",
        "tailwindcss": "^3.4.17",
        "terser": "^5.4.0",
        "typescript": "~5.6.2",
        "vite": "~5.2.0",
        "vite-plugin-checker": "^0.8.0",
        "vitest": "^0.34.6",
        "vue-tsc": "^2.1.10"
    },
    "description": "An Ionic project"
}
